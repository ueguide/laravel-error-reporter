{
    "name": "ueguide/laravel-error-reporter",
    "description": "A laravel provider to report errors over grpc",
    "authors": [
        {
            "name": "Aaron Kaczmarek",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0|~7.1",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
        "google/protobuf": "v3.6.1.3",
        "grpc/grpc": "^1.15"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4 || ~6.0",
        "orchestra/testbench": "~3.0",
        "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "Ueg\\ErrorReporter\\": "src",
            "UegProto\\LaravelErrorReporter\\Api\\": "UegProto/LaravelErrorReporter/Api",
            "GPBMetadata\\UegProto\\LaravelErrorReporter\\": "GPBMetadata/UegProto/LaravelErrorReporter"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ueg\\ErrorReporter\\Tests\\": "tests"
        }
    }
}
